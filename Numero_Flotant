flotant = float(input("Introdueix un numero flotant: "))
flotant2 = float(input("Introdueix un altre numero flotant: "))
if flotant>flotant2:
     print (flotant,"és més gran que", flotant2)
elif flotant<flotant2:
     print (flotant, "és més petit que", flotant2)
else:
     print (flotant, "és igual que", flotant2)
